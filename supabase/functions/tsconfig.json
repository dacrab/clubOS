{
    "compilerOptions": {
        "target": "ES2022",
        "strict": true,
        "skipLibCheck": true,
        "module": "ES2022",
        "moduleResolution": "bundler",
        "verbatimModuleSyntax": true,
        "resolveJsonModule": true,
        "lib": [
            "ES2022",
            "DOM"
        ],
        "types": []
    },
    "include": [
        "./**/*.ts",
        "./**/*.d.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}