{
	"name": "clubos-svelte",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && knip && eslint .",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"test": "SKIP_DB_TESTS=true vitest run",
		"test:db": "vitest run",
		"test:watch": "vitest",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"db:seed": "bun scripts/seed.ts",
		"db:reset": "supabase db reset && bun scripts/seed.ts"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@internationalized/date": "^3.11.0",
		"@lucide/svelte": "^0.563.1",
		"@playwright/test": "^1.58.1",
		"@sveltejs/adapter-vercel": "^6.3.1",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^25.2.0",
		"@vitest/coverage-v8": "^4.0.18",
		"@vitest/eslint-plugin": "^1.6.6",
		"bits-ui": "^2.15.5",
		"clsx": "^2.1.1",
		"eslint": "^9.39.2",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.3.0",
		"jsdom": "^28.0.0",
		"knip": "^5.83.0",
		"svelte": "^5.49.2",
		"svelte-check": "^4.3.6",
		"tailwind-merge": "^3.4.0",
		"tailwindcss": "^4.1.17",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.54.0",
		"vite": "^7.3.1",
		"vitest": "^4.0.18"
	},
	"dependencies": {
		"@supabase/ssr": "^0.8.0",
		"@supabase/supabase-js": "^2.94.1",
		"svelte-sonner": "^1.0.7"
	},
	"overrides": {
		"cookie": ">=0.7.0"
	}
}
