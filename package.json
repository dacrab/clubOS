{
  "name": "clubos-svelte",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && knip && eslint .",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:seed": "bunx tsx scripts/seed.ts",
    "db:reset": "supabase db reset && bunx tsx scripts/seed.ts",
    "db:restart": "supabase stop && supabase start"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@internationalized/date": "^3.10.0",
    "@lucide/svelte": "^0.553.0",
    "@sveltejs/adapter-vercel": "^6.1.1",
    "@sveltejs/kit": "^2.48.4",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.16",
    "@types/node": "^24.10.0",
    "bits-ui": "^2.14.2",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-svelte": "^3.13.0",
    "globals": "^16.5.0",
    "knip": "^5.67.1",
    "svelte": "^5.43.2",
    "svelte-check": "^4.3.3",
    "tailwind-merge": "^3.3.1",
    "tailwind-variants": "^3.1.1",
    "tailwindcss": "^4.1.16",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.1.12"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.78.0",
    "svelte-sonner": "^1.0.5"
  }
}
