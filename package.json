{
	"name": "clubos-svelte",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && knip && eslint .",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"test": "SKIP_DB_TESTS=true vitest run",
		"test:db": "vitest run",
		"test:watch": "vitest",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"db:seed": "bun scripts/seed.ts",
		"db:reset": "supabase db reset && bun scripts/seed.ts"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@internationalized/date": "^3.10.1",
		"@lucide/svelte": "^0.562.0",
		"@playwright/test": "^1.57.0",
		"@sveltejs/adapter-vercel": "^6.2.0",
		"@sveltejs/kit": "^2.49.3",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.18",
		"@types/d3-scale": "^4.0.9",
		"@types/node": "^25.0.3",
		"@vitest/coverage-v8": "^4.0.16",
		"@vitest/eslint-plugin": "^1.6.4",
		"bits-ui": "^2.14.4",
		"clsx": "^2.1.1",
		"eslint": "^9.39.2",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^17.0.0",
		"jsdom": "^27.4.0",
		"knip": "^5.78.0",
		"svelte": "^5.46.1",
		"svelte-check": "^4.3.5",
		"tailwind-merge": "^3.4.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.1.17",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.50.1",
		"vite": "^7.3.0",
		"vitest": "^4.0.16"
	},
	"dependencies": {
		"@supabase/ssr": "^0.8.0",
		"@supabase/supabase-js": "^2.89.0",
		"d3-scale": "^4.0.2",
		"layerchart": "^1.0.12",
		"svelte-sonner": "^1.0.7"
	},
	"overrides": {
		"cookie": ">=0.7.0"
	}
}
